debug('run_valrep');
$('#validation-content').html('<%= escape_javascript(@result.gsub(/\n/,'').html_safe) %>');
$('#validation-content').removeClass('hidden');
$('#validation-content').removeClass('fade');
$('#validation-content').show();

$('#link-to-run-valrep').html('Refresh the Validation Report');
$('#link-to-run-valrep').show();
$('#val-report-is-running-indicator').html("Ran at <%= Time.now.strftime('%H:%M:%S') %>");

