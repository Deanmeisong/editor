<% if search_result.has_parent? %>
  <% @child = 'child' %>
<% else %>
  <% @child = '' %>
<% end %>
<span class='review loader-name <%= @child %> <%= "#{search_result.record_type}-scientific-name" %>'>
<%= "#{search_result.scientific_name}" %>
</span>
<% if search_result.record_type == 'misapplied' then %>
  <span class="review loader-name misapplied-flag">misapplied</span>
<% end %>


 
