<div class="focus-details">
  <% if notice %> <p id="notice"><%= notice %></p> <% end %>

  <br>
  <%= @tree_element.simple_name %>
    <span class="search-result-operation operation-type-<%= params["tree-element-operation"] %>" >
    <%= params["tree-element-operation"] %>
    </span>
  <span class="pull-right"><%#= mapper_name_link(@name) %></span>

  <div>&nbsp;</div>

  <%= render partial: 'tree_elements/tabs/tabs' %>

  <%= render partial: "tree_elements/tabs/#{@tab}" %>

  <span class='server-response-time'><%= "(#{(Time.now - @start_time).round(2)}s)" if @start_time %></span>

</div><!-- /end .focus-details -->
